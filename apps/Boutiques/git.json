{
	"tool-version": "1.0.0",
	"name": "Git",
	"author": "Tanguy Duval",
	"description": [
		"Clone last extended spm12 version                                              ",
		"usage: git [--version] [--help] [-C <path>] [-c <name>=<value>]                ",
		"           [--exec-path[=<path>]] [--html-path] [--man-path] [--info-path]     ",
		"           [-p | --paginate | -P | --no-pager] [--no-replace-objects] [--bare] ",
		"           [--git-dir=<path>] [--work-tree=<path>] [--namespace=<name>]        ",
		"           <command> [<args>]                                                  ",
		"                                                                               "
	],
	"container-image": {
		"image": "ceregousa\/ubuntu-git",
		"type": "docker"
	},
	"inputs": [
		{
			"description": "Output Directory",
			"default-value": [
				"C:\\Users\\TONIC\\Desktop\\spm12"
			],
			"value-key": "[OUTPUT_DIRECTORY]",
			"type": "String",
			"optional": 0,
			"id": "output_directory",
			"name": "Output Directory"
		}
	],
	"command-line": "git clone --recurse-submodules https:\/\/github.com\/tanguyduval\/spm12.git [OUTPUT_DIRECTORY]"
}
